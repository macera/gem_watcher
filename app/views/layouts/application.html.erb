<!DOCTYPE html>
<html lang="ja">
<head>
  <title>GemWatcher</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render 'header' %>

    <div class="main ui container">
      <div class="row">
        <div class="column">
          <%= render 'messages' %>
          <%= yield %>
        </div>
      </div>
    </div>

</body>
</html>
